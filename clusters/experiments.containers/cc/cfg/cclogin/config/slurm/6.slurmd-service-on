#!/bin/bash

## $Header: $
## Source:
## @file vclogin/config/slurm/6.slurmd-service-on

VCLOAD=../../../provision/loader/shload.sh

if [ ! -f "${VCLOAD}" ] ; then
  echo "${0}: missing: ${VCLOAD}"
  exit 99
fi
source ${VCLOAD}

# if we're given an argument, append test output to it
declare -x OUT=${1:-""}

if [ -n "${OUT}" ] ; then
  touch ${OUT} || exit 1
fi

exec > >(tee -a "${OUT}") 2>&1

Rc ErrExit ${EX_SOFTWARE} "systemctl start slurmd"

trap '' 0
exit ${EX_OK}
